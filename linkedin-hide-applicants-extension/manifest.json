{
    "manifest_version": 3,
    "name": "Hide Applicants Count",
    "version": "0.0.0.3",
    "description": "Extension hides applicant count in the Job section of LinkedIn. Icons designed by Freepik",
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.linkedin.com/jobs/*"],
        "css": ["main.css"],
        "run_at": "document_idle"
      }
    ]
  }